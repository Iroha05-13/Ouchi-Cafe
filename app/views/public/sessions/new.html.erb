<div class="row">
  <h2 class="col-12">ログイン</h2>

  <%= form_with model: @user, url: user_session_path, id: 'new_user', class: 'new_user' do |f| %>
    <%= render "login", f: f %>
  <% end %>

  <p class="col-12 mt-5">新規登録は<%= render "public/shared/links" %>から</p>

  <p class="col-12 mb-5">ショップのログインは<%= link_to "こちら", new_shop_session_path %>から</p>
</div>
